@{
    ViewBag.Title = "Edição concluída";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Edição de candidato</h2>



<div class="form-horizontal">
    <h4>A edição foi efetuada com sucesso.</h4>
    <hr />
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            @Html.ActionLink("Visualizar candidato", "Read", new { id = Model }, new { @class = "btn btn-default" })
        </div>
    </div>
</div>

<div>
    @Html.ActionLink("Voltar a listagem", "List")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
